package com.web.Controller;

import com.web.entity.user.UandUid;
import com.web.services.UandUidService;
import com.web.services.UserService;
import com.web.services.servicesImpl.UserServiceImpl;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.RequestMapping;
import org.apache.ws.commons.schema.XmlSchemaAnnotated;

import java.util.List;

/**
 * @ProjectName: MavenModule
 * @Package: com.web.Controller
 * @ClassName: ControllerUser
 * @Author: 废材
 * @Description: user的前台(显示层)
 * @Date: 2019/3/2 16:58
 * @Version: 1.0
 */

@Controller("ControllerUser")
public class ControllerUser {
    @Autowired
    UandUidService uandUidService;
    @RequestMapping(value = "/allUser")
    public String UserController(Model model, Integer id){
        List<UandUid> uandUids = uandUidService.selecrUUID(1);
        model.addAttribute("uandUids",uandUids);
        return "allUser";
    }

}
